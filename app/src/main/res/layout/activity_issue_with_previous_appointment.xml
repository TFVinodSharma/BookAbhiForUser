<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/whiteblue"
    tools:context="tris.bookabhiforuser.IssueWithPreviousAppointment">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name"
            android:padding="5dp"
            android:layout_marginLeft="5dp"
            android:textSize="@dimen/font_size1"
            android:textStyle="bold"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Query Type"
            android:padding="5dp"
            android:layout_marginLeft="10dp"
            android:textSize="@dimen/font_size1"
            android:textStyle="bold"
            />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mode"
            android:padding="5dp"
            android:layout_marginLeft="10dp"
            android:textSize="@dimen/font_size1"
            android:textStyle="bold"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Probable Time"
            android:layout_marginLeft="10dp"
            android:padding="5dp"
            android:textSize="@dimen/font_size1"
            android:textStyle="bold"
            />

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Vinod"
            android:padding="5dp"
            android:layout_marginLeft="5dp"
            android:textSize="@dimen/fab_margin"
            android:textStyle="bold"
            />
        <TextView
            android:layout_width="95dp"
            android:layout_height="wrap_content"
            android:text="Time Management"
            android:layout_marginLeft="8dp"
            android:padding="5dp"
            android:textSize="@dimen/fab_margin"
            android:textStyle="bold"
            />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Call"
            android:layout_marginLeft="10dp"
            android:padding="5dp"
            android:textSize="@dimen/fab_margin"
            android:textStyle="bold"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="7:00 to 9:00 PM"
            android:padding="5dp"
            android:layout_marginLeft="10dp"
            android:textSize="@dimen/fab_margin"
            android:textStyle="bold"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Help"
            android:padding="5dp"
            android:id="@+id/issuehelp1"
            android:layout_marginLeft="10dp"
            android:textSize="@dimen/fab_margin"
            android:textStyle="bold"
            />


    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Vinod"
            android:padding="5dp"
            android:layout_marginLeft="5dp"
            android:textSize="@dimen/fab_margin"
            android:textStyle="bold"
            />
        <TextView
            android:layout_width="95dp"
            android:layout_height="wrap_content"
            android:text="Time Management"
            android:layout_marginLeft="8dp"
            android:padding="5dp"
            android:textSize="@dimen/fab_margin"
            android:textStyle="bold"
            />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Call"
            android:layout_marginLeft="10dp"
            android:padding="5dp"
            android:textSize="@dimen/fab_margin"
            android:textStyle="bold"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="7:00 to 9:00 PM"
            android:padding="5dp"
            android:layout_marginLeft="10dp"
            android:textSize="@dimen/fab_margin"
            android:textStyle="bold"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Help"
            android:padding="5dp"
            android:id="@+id/issuehelp2"
            android:layout_marginLeft="10dp"
            android:textSize="@dimen/fab_margin"
            android:textStyle="bold"
            />


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Rama"
            android:padding="5dp"
            android:layout_marginLeft="5dp"
            android:textSize="@dimen/fab_margin"
            android:textStyle="bold"
            />
        <TextView
            android:layout_width="95dp"
            android:layout_height="wrap_content"
            android:text="Subject Specific"
            android:layout_marginLeft="10dp"
            android:padding="5dp"
            android:textSize="@dimen/fab_margin"
            android:textStyle="bold"
            />


        <TextView
            android:layout_width="48dp"
            android:layout_height="wrap_content"
            android:text="Vedio Call"
            android:padding="4dp"
            android:layout_marginLeft="6dp"
            android:textSize="@dimen/fab_margin"
            android:textStyle="bold"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="7:00 to 9:00 PM"
            android:padding="5dp"
            android:textSize="@dimen/fab_margin"
            android:textStyle="bold"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Help"
            android:padding="5dp"
            android:id="@+id/issuehelp3"
            android:layout_marginLeft="10dp"
            android:textSize="@dimen/fab_margin"
            android:textStyle="bold"
            />


    </LinearLayout>


</LinearLayout>
